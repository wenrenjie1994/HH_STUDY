<?xml version="1.0" encoding="UTF-8"?>  
 <web-app>
 <servlet>
  <servlet-name>login</servlet-name>
  <servlet-class>cn.bigruili.server.LoginServlet</servlet-class>
 </servlet>
   <servlet>
  <servlet-name>reg</servlet-name>
  <servlet-class>cn.bigruili.server.RegisterServlet</servlet-class>
 </servlet>   
  <servlet>
   <servlet-name>others</servlet-name>
  <servlet-class>cn.bigruili.server.OthersServlet</servlet-class>
 </servlet>  
 <servlet-mapping>
  <servlet-name>login</servlet-name>
  <url-pattern>/login</url-pattern> 
  <url-pattern>/g</url-pattern> 
 </servlet-mapping>  
 <servlet-mapping>
  <servlet-name>reg</servlet-name>
  <url-pattern>/reg</url-pattern> 
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>others</servlet-name>
  <url-pattern>/o</url-pattern> 
  <url-pattern>/others</url-pattern> 
 </servlet-mapping>
 </web-app>
